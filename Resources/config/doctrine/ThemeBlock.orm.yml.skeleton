# 
Application\PrestaCMS\CoreBundle\Entity\ThemeBlock:
  type: entity
  table: prestacms_theme_block
  id:
    id:
      type: integer
      generator: { strategy: AUTO }
  oneToMany:
    translations:
      targetEntity: PrestaCMS\CoreBundle\Entity\ThemeBlockTranslation
      mappedBy: object
      cascade: ["persist", "remove"]
    
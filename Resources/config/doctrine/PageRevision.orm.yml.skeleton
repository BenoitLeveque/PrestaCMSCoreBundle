# src/Acme/StoreBundle/Resources/config/doctrine/BaseWebsite.orm.yml
Application\PrestaCMS\CoreBundle\Entity\PageRevision:
  type: entity
  table: prestacms_page_revision
  id:
    id:
      type: integer
      generator: { strategy: AUTO }
  oneToMany:
    blocks:
      targetEntity: Application\PrestaCMS\CoreBundle\Entity\PageBlock
      mappedBy: pageRevision  
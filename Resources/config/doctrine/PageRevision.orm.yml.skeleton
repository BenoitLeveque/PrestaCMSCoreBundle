# src/Acme/StoreBundle/Resources/config/doctrine/BaseWebsite.orm.yml
Application\Presta\CMSCoreBundle\Entity\PageRevision:
  type: entity
  table: prestacms_page_revision
  id:
    id:
      type: integer
      generator: { strategy: AUTO }
  oneToMany:
    blocks:
      targetEntity: Application\Presta\CMSCoreBundle\Entity\PageRevisionBlock
      mappedBy: pageRevision  
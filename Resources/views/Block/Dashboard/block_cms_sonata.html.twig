<table class="table table-bordered table-striped sonata-ba-list">
    <thead>
    <tr>
        <th colspan="3">{% trans from 'PrestaCMSCoreBundle' %}dashboard.block.cms.title{% endtrans %}</th>
    </tr>
    </thead>

    <tbody>
    <tr>
        <td class="sonata-ba-list-label">{% trans from 'PrestaCMSCoreBundle' %}dashboard.block.website.title{% endtrans %}</td>
        <td>
            <a href="{{ websiteAdmin.generateUrl('list') }}" class="btn btn-small">
                <i class="icon-list"></i>{%- trans from 'SonataAdminBundle' %}link_list{% endtrans -%}
            </a>
        </td>
    </tr>
    <tr>
        <td class="sonata-ba-list-label">{% trans from 'PrestaCMSCoreBundle' %}dashboard.block.theme.title{% endtrans %}</td>

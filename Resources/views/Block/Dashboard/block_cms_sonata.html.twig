            <a href="{{ path('presta_cms_theme') }}" class="btn btn-small">
                <i class="icon-list"></i>{%- trans from 'SonataAdminBundle' %}link_list{% endtrans -%}
            </a>
        </td>
    </tr>
    <tr>
        <td class="sonata-ba-list-label">{% trans from 'PrestaCMSCoreBundle' %}dashboard.block.page.title{% endtrans %}</td>
        <td>
            <a href="{{ path('presta_cms_page_edit') }}" class="btn btn-small">
                <i class="icon-list"></i>{%- trans from 'SonataAdminBundle' %}link_list{% endtrans -%}
